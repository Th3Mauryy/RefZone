{
  "version": 2,
  "builds": [
    {
      "src": "mi-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "mi-backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/mi-backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/mi-frontend/dist/$1"
    }
  ]
}
